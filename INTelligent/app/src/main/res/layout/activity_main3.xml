<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context="com.example.sheetal.myapplication.Main3Activity">

    <TextView
        android:id="@+id/tv3"
        android:layout_width="250dp"
        android:layout_height="57dp"
        android:layout_marginBottom="41dp"
        android:text="          Your Score"
        android:textColor="#ffffff"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="129dp"
        android:layout_above="@+id/tv4"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="40sp"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="246dp"
        android:layout_marginBottom="66dp"
        android:layout_above="@+id/tv5"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/again"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start Page"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="343dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="57dp" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#ffffff"
        android:textSize="40dp"
        android:layout_marginBottom="83dp"
        android:layout_above="@+id/again"
        android:layout_alignLeft="@+id/tv4"
        android:layout_alignStart="@+id/tv4" />
</RelativeLayout>
